    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@unocss/eslint-config": "^66.1.3",
    "@unocss/extractor-svelte": "^66.1.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.9.0",
    "globals": "^16.2.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.33.11",
